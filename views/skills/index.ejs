<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <title>Express Dev Skills</title>
</head>
<body>
  <h1>Skills</h1>
  <ul>
    <% skills.forEach(function(s) { %>
      <li>
        <%= s.dev %>
        <ul>
          HTML: <%= s.html ? 'Yup' : 'Nope' %>
        </ul> 
        <ul>
          CSS: <%= s.css ? 'Yup' : 'Nope' %>
        </ul> 
        <ul>
          JS: <%= s.js ? 'Yup' : 'Nope' %>
        </ul> 
        <ul>
          Express: <%= s.express ? 'Yup' : 'Nope' %>
        </ul> 
      </li>
    <% }); %>
  </ul>
</body>
</html>