<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <title>Express Dev Skills</title>
</head>
<body>
  <a href="/">Home</a> |
  <a href="/skills/new">Add Skill</a>
  <h1>Skills as of <%= time %></h1>
  <ul>
    <% skills.forEach(function(s) { %>
    <li>
    <a href="/skills/<%= s.id %>"><%= s.skill %></a>
    <form action="/skills/<%= s.id %>?_method=DELETE"
      class="delete-form" method="POST">
      <button type="submit">X</button>
    </form>
    </li>
    <% }); %>
  </ul>
  </ul>
</body>
</html>